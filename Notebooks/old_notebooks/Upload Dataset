me}\")\n","\n","    # 保存到子文件夹\n","    file_path = os.path.join(subfolder, file_name)\n","    df.to_csv(file_path, index=False)\n","\n","    # 将数据加载到Dataset对象中\n","    dataset_split = Dataset.from_pandas(df)\n","    dataset_dict[split] = dataset_split\n","\n","# 推送到Hugging Face Hub\n","dataset_dict.push_to_hub(\"Mingda/MBPP-Translated\")\n","\n","# 如果需要将数据集设置为私有，可以使用以下代码\n","# dataset_dict.push_to_hub(\"Mingda/private_MBPP-Translated\", private=True)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":533,"referenced_widgets":["ffdfc23e67dd4cd285508fce00b1a009","39c8901d0c924e9cbee1a90c7ddbd6c4","08b2cb7a859c4e698bc8c983372e6f1e","9a153d049dc9451080b21a33ad77ef00","575b70244d0049b39716e0ca169e02b5","0351f5022c824a98bdf1864fe247d4fc","4b91455a28ae4f46816c4d6de516680b","f7da97a4732b42d5af00cdb5768ff273","7adeb06fc09c4e33bc66acb66253c6e9","ae4c7de30dc641539a9b0d0b9946f5bd","66ca2bb7fc0b4073bb2258a1687744d5","551d80dab01340f2a9fbc76f858defe2","db0938cd0f094696940c91f1e9bae6ec","27d9df312168447bb650f931faf3d145","b0bf82e8a12041229bdc965b6772975a","4dcc72a014d7483a8f2690aad377301f","0acfaeccdbb141158fbac644d7ab1549","80f03f429c6349148bba4c1b4905209a","f1b2ed86ee154ad495bc2d9429a8f016","9694d64407684084a17dc42946fdd1dc","f7652aee3a904869ad6b1864e54e0687","1470d94756b6405aa971228b609bd0f5","da92322eab394257ac62f1b14728e3a0","e0ada835f4664af98eae8f3575c76da2","cd25c9f762d44d29a8f726f34ba6ffa8","5c984b248b5d467aac3f617bf549ada2","1e2833c5d44f42548edeb5d028e3063b","144c532d0fe842b0b14ed9d1cffea661","eb6ba780dee44079b97b866c59863a4a","99e1a5423de74bf78666227ab007c77a","e6ec49fd31fa432bb2be41a0f54f547d","3b8116204f984c029548e6ba22fbb919","22d4bb3075274255a8862f547ebc7f34","74e7c558c1bf4485b465db6175684eb4","8ee4f2c302f342dd8adc5489e79b62c5","b7113dae903f4d7399539da6ddbb389b","2ed43d0b74d244679c6cb8b4409e4025","0d2f87282be04493a99e37851e597176","5c29b6baea3246ffa4c6cab534c4c101","29aabc5159714987805457bcf46e008f","87b06711cf3b410694a912f6861c1d95","4d9ba8ae17194