r expression patterns for the specified scenarios\n","patterns = [\n","    re.compile(r'(?:\\'\\'\\'|\\\"\\\"\\\")(.*?)(?:\u003e\u003e\u003e)', re.DOTALL),  # before \u003e\u003e\u003e\n","    re.compile(r'(?:\\'\\'\\'|\\\"\\\"\\\")(.*?)(?:\\'\\'\\'|\\\"\\\"\\\")', re.DOTALL),  # between triple quotes\n","    re.compile(r'(?:\\'\\'\\'|\\\"\\\"\\\")(.*?)(?=Example:)', re.DOTALL),  # before 'Example:'\n","    re.compile(r'(?:\\'\\'\\'|\\\"\\\"\\\")(.*?)(?=Examples)', re.DOTALL),  # before 'Examples:'\n","    re.compile(r'(?:\\'\\'\\'|\\\"\\\"\\\")(.*?)(?=Example 1:)', re.DOTALL),  # before 'Example 1:'\n","    re.compile(r'(?:\\'\\'\\'|\\\"\\\"\\\")(.*?)(?=for example:)', re.DOTALL),  # before 'for example:'\n","    re.compile(r'(?:\\'\\'\\'|\\\"\\\"\\\")(.*?)(?=For Example)', re.DOTALL),  # before 'For Example:'\n","    re.compile(r'(?:\\'\\'\\'|\\\"\\\"\\\")(.*?)(?=For example:)', re.DOTALL),  # before 'For example:'\n","    re.compile(r'(?:\\'\\'\\'|\\\"\\\"\\\")(.*?)(?=example:)', re.DOTALL)  # before 'example:'\n","]\n","\n","def clean_text(text):\n","    # Insert a period before uppercase letters that follow newline and spaces,\n","    # if not preceded by a period and not at the beginning of the text\n","    cleaned_text = re.sub(r'(?\u003c!\\.)\\n\\s+([A-Z])', r'. \\1', text.strip())\n","    # Remove multiple periods even if they are separated by spaces\n","    cleaned_text = re.sub(r'\\.\\s*\\.+', '.', cleaned_text)\n","    if cleaned_text.startswith('. '):\n","        cleaned_text = cleaned_text[2:]\n","    return ' '.join(cleaned_text.split())\n","\n","cnt = 1\n","\n","with jsonlines.open(input_filename) as reader, jsonlines.open(output_filename, mode='w') as writer:\n","    for obj in reader:\n","        print(cnt)\n","        print(obj['prompt'])\n","        print(\"Result:\")\n","        # Splitting the prompt by 'def ' to separate different functions\n","        parts = obj['prompt'].split('def ')[1:]\n","        english_portions = []\n","\n","        for part in parts:\n","            # Prepending 'def ' to keep the original content structure\n","            part = 'def ' + part\n","\n","            matches = []\n","            for pattern in patterns:\n","                match = pattern.search(part)\n","                if match and match.group(1).strip():  # Check if the matched content is not empty\n","                  matches.append(clean_text(match.group(1)))\n","\n","            if matches:\n","                portion = min(matches, key=len)\n","                if not portion.endswith('.'):\n","                    portion += '.'\n","                english_portions.append(portion)\n","\n","        if english_portions:\n","            obj['english_portion'] = ' '.join(english_portions)\n","            obj['english_portion'] = clean_text(obj['english_portion'])\n","            print(obj['english_portion'])\n","        else:\n","            print(\"error\")\n","            print(obj['prompt'])\n","            break\n","\n","        writer.write(obj)\n","        cnt += 1\n","\n","\n","\n","\n","print(f\"Processed file written to {output_filename}\")\n"]}],"metadata":{"colab":{"authorship_tag":"ABX9TyNpihFQf9jm+uQR9gEoRLeV","name":"","version":""},"kernelspec":{"display_name":"Python 3","name":"python3"},"language_info":{"name":"python"}},"nbformat":4,"nbformat_minor":0}